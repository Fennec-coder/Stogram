<%= search_form_for @query do |f| %>

  <%= f.label :username_cont %>
  <%= f.search_field :username_cont %>

  <%= f.submit %>
<% end %><br>

<% @users.each do |user| %>
  <%= link_to (image_tag user.avatar_url, :id => "avatar-profile-small"), user_path(user) %>
  <%= link_to "#{user.username}", user_path(user) %><br>
<% end %>